{
  "schemaVersion": 1,
  "id": "laglessboot",
  "version": "${version}",

  "name": "Lagless Boot",
  "description": "Reduce threads used during Minecraft initialization so it doesn't freeze other applications.\nProbably most effective for computers with 8 thread CPUs.",
  "authors": [
    "UltimateBoomer"
  ],
  "contact": {
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "GNU General Public License",
  "icon": "assets/laglessboot/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.ultimateboomer.laglessboot.LaglessBoot"
    ]
  },
  "mixins": [
    "laglessboot.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "flamingo": "*"
  }
}
