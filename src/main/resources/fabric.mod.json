{
  "schemaVersion": 1,
  "id": "laglessboot",
  "version": "${version}",

  "name": "Lagless Boot",
  "description": "Make Minecraft not slow down your entire computer when loading up.",
  "authors": [
    "UltimateBoomer"
  ],
  "contact": {
    "homepage": "https://github.com/UltimateBoomer",
    "sources": "https://github.com/UltimateBoomer/laglessboot-fabric"
  },

  "license": "MIT License",
  "icon": "assets/laglessboot/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "com.ultimateboomer.laglessboot.LaglessBoot"
    ],
    "modmenu": [
      "com.ultimateboomer.laglessboot.config.LaglessBootModMenu"
    ]
  },
  "mixins": [
    "laglessboot.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "flamingo": "*"
  }
}
